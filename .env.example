# Django Settings
SECRET_KEY=your-secret-key-here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database Oracle Configuration
DB_ENGINE=django.db.backends.oracle
DB_NAME=XEPDB1
DB_USER=your-oracle-username
DB_PASSWORD=your-oracle-password
DB_HOST=localhost
DB_PORT=1521
DB_SERVICE_NAME=XEPDB1

# Oracle Production Tables Configuration
ORACLE_FORM_TABLE=AC_FORMULARIO_GLOBAL_PIR
ORACLE_EST_NUEVA_TABLE=AC_ESTRUCTURA_NUEVA_PIR
ORACLE_EST_RETIRADA_TABLE=AC_ESTRUCTURA_RETIRADA_PIR
ORACLE_PROYECTO_TABLE=AC_PROYECTO_INFO_PIR
ORACLE_FORM_SEQ=AC_FORM_SEQ_PIR
ORACLE_EST_NUEVA_SEQ=AC_EST_NUEVA_SEQ_PIR
ORACLE_EST_RET_SEQ=AC_EST_RET_SEQ_PIR
ORACLE_PROY_SEQ=AC_PROY_SEQ_PIR
ORACLE_TABLE_PREFIX=AC_
ORACLE_TABLE_SUFFIX=PIR

# Oracle Client Configuration (opcional para modo thick)
# ORACLE_CLIENT_PATH=/path/to/oracle/instantclient

# Media and Static Files
MEDIA_URL=/media/
STATIC_URL=/static/

# Security Settings
SECURE_BROWSER_XSS_FILTER=True
SECURE_CONTENT_TYPE_NOSNIFF=True
X_FRAME_OPTIONS=DENY
