# Django Settings
SECRET_KEY=your-secret-key-here
DEBUG=True
ALLOWED_HOSTS=localhost,127.0.0.1

# Database Oracle Configuration
DB_ENGINE=django.db.backends.oracle
DB_NAME=XEPDB1
DB_USER=your-oracle-username
DB_PASSWORD=your-oracle-password
DB_HOST=localhost
DB_PORT=1521
DB_SERVICE_NAME=XEPDB1

# Oracle Client Configuration (opcional para modo thick)
# ORACLE_CLIENT_PATH=/path/to/oracle/instantclient

# Cloudinary Configuration
CLOUDINARY_CLOUD_NAME=your-cloudinary-cloud-name
CLOUDINARY_API_KEY=your-cloudinary-api-key
CLOUDINARY_API_SECRET=your-cloudinary-api-secret

# Media and Static Files
MEDIA_URL=/media/
STATIC_URL=/static/

# Security Settings
SECURE_BROWSER_XSS_FILTER=True
SECURE_CONTENT_TYPE_NOSNIFF=True
X_FRAME_OPTIONS=DENY
